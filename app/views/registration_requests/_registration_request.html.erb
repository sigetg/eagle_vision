<div id="<%= dom_id registration_request %>">
  <h3>
    <strong>Name:</strong>
    <%= registration_request.name %>
  </h3>
  <p>
    <strong>Status:</strong>
    <%= registration_request.stateKey %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= registration_request.descr %>
  </p>

  <p>
    <strong>Effective Date:</strong>
    <%= registration_request.effectiveDate %>
  </p>

  <p>
    <strong>Expiration Date:</strong>
    <%= registration_request.expirationDate %>
  </p>

  <p>
    <strong>Creator:</strong>
    <%= registration_request.person_id %>
  </p>

  <p>
    <strong>Term:</strong>
    <%= registration_request.term_id %>
  </p>

</div>
