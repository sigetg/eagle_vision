    <%= form.label :typeKey, style: "display: block" do %>
      Type Key: Waitlist
    <% end %>
    <%= form.hidden_field :typeKey, value: registration_request.typeKey %>
  </div>

  <% if current_user.has_role?(:admin) %>
    <div>
      <%= form.label :stateKey, style: "display: block" %>
      <%= form.text_field :stateKey, value: registration_request.stateKey %>
    </div>
  <% else %>
